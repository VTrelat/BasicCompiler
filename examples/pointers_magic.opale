int main() {
    // locale variables are stored on the stack (rbp-8, rbp-16, ...)
    // (the offset depends on the size)
    // sizes:
    //  int:  8
    //  char: 1
    // a: rbp-8
    int a = 255;
    // b: rbp-16
    int b;
    // c: rbp-24 = rbp-8-16
    int c = 0;
    int * p;
    // p points to a
    p = &a;
    printf(*p);
    printf(a);
    p = p - 16;
    // p now points to c
    printf(p);
    printf(*p);
    printf(c);
    // we can manipulate the value of c through p
    *p = 42;
    printf(c);
    giveMeBack 0;
}